{
  "expo": {
    "name": "Thorium Mobile",
    "description": "A companion mobile application for Thorium.",
    "slug": "thorium-mobile",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "us.fyreworks.thoriumMobile"
    },
    "android": {
      "package": "us.fyreworks.thoriumMobile",
      "versionCode": 2
    },
    "githubUrl": "https://github.com/Thorium-Sim/thorium-mobile"
  }
}
